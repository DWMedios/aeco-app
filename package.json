{
  "name": "aeco",
  "description": "AECO APP",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/franciscocamposchimal",
  "author": {
    "name": "Franko Campos",
    "email": "camposfranciscoing@gmail.com"
  },
  "keywords": [],
  "license": "ISC",
  "main": "electron.mjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "dev": "webpack serve",
    "electron": "wait-on http://localhost:3000 && electron .",
    "start:dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "dist": "electron-builder",
    "build:react": "react-scripts build",
    "eject:react": "react-scripts eject",
    "build:raspi": "electron-builder --armv7l",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "productName": "aeco-app",
    "appId": "com.example.aeco-app",
    "buildVersion": "1.0.0",
    "artifactName": "${productName}-${version}.${ext}",
    "directories": {
      "output": "release"
    },
    "files": [
      "electron.mjs",
      "build/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "armv7l"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "armv7l"
          ]
        }
      ],
      "category": "Utility"
    },
    "extraMetadata": {
      "main": "electron.mjs"
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "7.1.2",
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "4.0.0",
    "webpack": "5.92.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
